{
  "name": "cmdk-launch-kit",
  "license": "MIT",
  "version": "0.1.0",
  "description": "CMDK Launch Kit",
  "files": [
    "src/**/*.sol"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMDKONE/cmdk-launch-kit.git"
  },
  "scripts": {
    "clean": "forge clean && forge build",
    "test": "npm run clean && forge test",
    "test:logs": "forge test -vvvv",
    "docs": "forge doc",
    "deploy:testnet": "forge script script/DeployTestnet.s.sol:DeployTestnet --rpc-url sepolia --broadcast --verify -vvvv",
    "deploy:cmdk:testnet": "forge script script/DeployCMDKLaunchKit.s.sol:DeployCMDKLaunchKit --rpc-url sepolia --broadcast --verify -vvvv",
    "deploy:claiming:testnet": "forge script script/DeployClaimAndStake.s.sol:DeployClaimAndStake --rpc-url sepolia --broadcast --verify -vvvv",
    "deploy:cmdk:mainnet": "forge script script/DeployCMDKLaunchKit.s.sol:DeployCMDKLaunchKit --rpc-url mainnet --broadcast --verify -vvvv",
    "deploy:claiming:mainnet": "forge script script/DeployClaimAndStake.s.sol:DeployClaimAndStake --rpc-url mainnet --broadcast --verify -vvvv"
  }
}
